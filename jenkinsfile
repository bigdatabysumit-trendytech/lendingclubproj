pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
               sh 'pip install pipenv'
               sh 'pipenv --python python3 sync'
            }
        }
        stage('Test') {
            steps {
               echo 'test completed successfullyyy'
            }
        }
        stage('Package') {
            steps {
               echo 'package completed successfully'
            }
        }
	
    }
}
